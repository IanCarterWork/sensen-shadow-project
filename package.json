{
  "name": "sensen-shadow",
  "version": "0.0.1",
  "description": "Web template runtime",
  "main": "app/index.ts",
  "scripts": {
    "mount": "node sensen/mount && tsc",
    "watch": "tsc -watch",
    "serve": "tsc && npx webpack serve --hot --config ./.sensen/webpack.config.js ",
    "build": "tsc && webpack --config ./.sensen/webpack.config.js",
    "publish": "tsc && webpack --config ./.sensen/webpack.config.js --mode=production"
  },
  "keywords": [
    "html",
    "css",
    "js",
    "typescript",
    "template",
    "sensen",
    "shadow",
    "template",
    "engine",
    "web",
    "runtime"
  ],
  "author": "Yannick GOBOU",
  "license": "ISC",
  "dependencies": {
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "node-loader": "^2.0.0",
    "postcss-loader": "^6.2.1",
    "sensen-hinata": "^0.0.24",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "devDependencies": {
    "ignore-loader": "^0.1.2"
  }
}
